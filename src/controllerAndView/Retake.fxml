<AnchorPane fx:id="retakeScene" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllerAndView.RetakeController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ffffff00" height="330.0" layoutX="20.0" layoutY="150.0" stroke="#374356" strokeType="INSIDE" strokeWidth="2.0" width="660.0" />
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="130.0" prefWidth="700.0" style="-fx-background-color: #374356;" text="Retake" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="45.0" />
         </font>
      </Label>
      <Text fill="#374356" layoutX="286.0" layoutY="198.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select the Food:" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Text>
      <Label fx:id="numError" alignment="CENTER" layoutX="284.0" layoutY="292.0" textFill="RED">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="foodChoiceBox" layoutX="217.0" layoutY="250.0" prefHeight="27.0" prefWidth="265.0" style="-fx-background-color: transparent; -fx-border-color: #374356;" />
      <Button fx:id="retakeButton" alignment="CENTER" contentDisplay="CENTER" layoutX="314.0" layoutY="379.0" mnemonicParsing="false" onAction="#retaking" style="-fx-background-color: #374356;" text="Retake" textFill="WHITE">
