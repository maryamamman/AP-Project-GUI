      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="115.0" layoutY="415.0" mnemonicParsing="false" onAction="#exit" prefHeight="30.0" prefWidth="80.0" style="-fx-background-color: #374356;" text="EXIT" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="506.0" layoutY="415.0" mnemonicParsing="false" onAction="#back" prefHeight="30.0" prefWidth="80.0" style="-fx-background-color: #374356;" text="BACK" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="waitButton" alignment="CENTER" contentDisplay="CENTER" layoutX="306.0" layoutY="379.0" mnemonicParsing="false" onAction="#waitList" style="-fx-background-color: #374356;" text="Wait List" textFill="WHITE" visible="false">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
