<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="showDemandsScene" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllerAndView.ShowDemandsController">
   <children>
      <Label alignment="CENTER" prefHeight="130.0" prefWidth="700.0" style="-fx-background-color: #374356;" text="Reports" textFill="WHITE">
         <font>
            <Font name="System Bold" size="45.0" />
         </font>
      </Label>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#2d445900" height="330.0" layoutX="20.0" layoutY="150.0" stroke="#374356" strokeType="INSIDE" strokeWidth="2.0" width="660.0" />
      <RadioButton fx:id="breakfast" contentDisplay="CENTER" layoutX="181.0" layoutY="232.0" mnemonicParsing="false" onAction="#foodType" text="Breakfast" textAlignment="CENTER" textFill="#374356">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="food1" />
         </toggleGroup>
      </RadioButton>
      <TextField fx:id="dayGetter" layoutX="252.0" layoutY="164.0" promptText="Day" style="-fx-background-color: transparent; -fx-border-color: #374356; -fx-border-width: 0px 0px 2px 0px;">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </TextField>
      <Label fx:id="information" alignment="CENTER" contentDisplay="CENTER" layoutX="152.0" layoutY="315.0" prefHeight="144.0" prefWidth="410.0" textAlignment="CENTER" textFill="#374356">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <RadioButton fx:id="lunch" contentDisplay="CENTER" layoutX="321.0" layoutY="232.0" mnemonicParsing="false" onAction="#foodType" text="Lunch" textAlignment="CENTER" textFill="#374356" toggleGroup="$food1">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="dinner" contentDisplay="CENTER" layoutX="437.0" layoutY="232.0" mnemonicParsing="false" onAction="#foodType" text="Dinner" textAlignment="CENTER" textFill="#374356" toggleGroup="$food1">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </RadioButton>
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="42.0" layoutY="432.0" mnemonicParsing="false" onAction="#exit" prefHeight="30.0" prefWidth="80.0" style="-fx-background-color: #374356;" text="EXIT" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="579.0" layoutY="432.0" mnemonicParsing="false" onAction="#back" prefHeight="30.0" prefWidth="80.0" style="-fx-background-color: #374356;" text="BACK" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Label fx:id="numError" alignment="CENTER" layoutX="279.0" layoutY="207.0" prefHeight="20.0" prefWidth="157.0" textFill="RED">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="243.0" layoutY="277.0" mnemonicParsing="false" onAction="#distribute" prefHeight="45.0" prefWidth="100.0" style="-fx-background-color: #374356;" text="Distribute" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="364.0" layoutY="277.0" mnemonicParsing="false" onAction="#foodDemands" prefHeight="45.0" prefWidth="100.0" style="-fx-background-color: #374356;" text="Food&#10;Demands" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
